{
  "name": "pytorch-block",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=client\"",
    "build": "npm run build --workspace=server && npm run build --workspace=client",
    "start": "concurrently \"npm run start --workspace=server\" \"npm run start --workspace=client\"",
    "start:server": "npm run start --workspace=server",
    "start:client": "npm run start --workspace=client"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "concurrently": "^8.2.2"
  },
  "description": "A full-stack React + TypeScript + Node.js application for managing and sharing code modules.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "ansi-regex": "^5.0.1",
    "ansi-styles": "^4.3.0",
    "chalk": "^4.1.2",
    "client": "^0.0.0",
    "cliui": "^8.0.1",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "date-fns": "^2.30.0",
    "emoji-regex": "^8.0.0",
    "escalade": "^3.2.0",
    "get-caller-file": "^2.0.5",
    "has-flag": "^4.0.0",
    "is-fullwidth-code-point": "^3.0.0",
    "lodash": "^4.17.21",
    "require-directory": "^2.1.1",
    "rxjs": "^7.8.2",
    "server": "^1.0.0",
    "shell-quote": "^1.8.3",
    "spawn-command": "^0.0.2",
    "string-width": "^4.2.3",
    "strip-ansi": "^6.0.1",
    "supports-color": "^8.1.1",
    "tree-kill": "^1.2.2",
    "tslib": "^2.8.1",
    "wrap-ansi": "^7.0.0",
    "y18n": "^5.0.8",
    "yargs": "^17.7.2",
    "yargs-parser": "^21.1.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
